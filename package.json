    "lint:fix": "npm run lint --fix",
    "format": "prettier --write \"src/**/*.tsx\" \"src/**/*.ts\" \"*.js\"",
    "format-md": "prettier --write \"content/**/*.md\"",
    "gentags": "genCtags.bat",
    "build-storybook": "build-storybook",
    "postinstall": "node postinstall.js"
